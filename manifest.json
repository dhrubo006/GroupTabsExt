{
    "manifest_version": 3,
    "name": "Tab Group Extension",
    "version": "0.0",
    "description": "Simple Chrome extension to group tabs together.",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "icon48.png"
    }
}